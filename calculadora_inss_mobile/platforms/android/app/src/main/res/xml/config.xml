<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.joaocabral.calculadorainss" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>CalculadoraINSS</name>
    <description>
        Sistema de Calculadora INSS usando Cordova.
    </description>
    <author email="joaoamunizshura@gmail.com" href="https://github.com/joaocabraldev/calculadora_inss">
        João Antônio Cabral
    </author>
    <content src="index.html" />
    <icon src="www/icons/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
