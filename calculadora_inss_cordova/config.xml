<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.joaocabral.calculadorainss" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CalculadoraINSS</name>
    <description>
        Sistema de Calculadora INSS usando Cordova.
    </description>
    <author email="joaoamunizshura@gmail.com" href="https://github.com/joaocabraldev/calculadora_inss">
        João Antônio Cabral
    </author>
    <content src="index.html" />
    <icon src="www/icons/icon.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
